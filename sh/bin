#!/usr/bin/env zsh
# YES_ZSH

bin() {
    local a="$(($@))"
    let 'a=(((((((((((((a + 0x0FFFFF80) & 0x1000007F) + 0x00FFFFC0) & 0x1100003F) + 0x000FFFE0) & 0x1110001F) + 0x0000FFF0) & 0x1111000F) + 0x00000FF8) & 0x11111007) + 0x000000FC) & 0x11111103) + 0x0000000E) & 0x11111111'
    printf "%08X\n" "$a"
}

[ "${SOURCING:-0}" -gt 0 ] || bin "$@"
